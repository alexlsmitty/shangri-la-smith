{
  "name": "shangria-la",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "migrate": "node migrate-to-supabase-fixed.js",
    "start": "node server.js",
    "vercel-install": "npm install terser",
    "vercel-build": "npm run vercel-install && echo 'Building for Vercel...' && npx vite build --config vite.config.vercel.js",
    "check-gh-pages": "node scripts/check-gh-pages.js",
    "check-deployment": "vite build --debug --mode production && vite preview",
    "verify-deployment": "node --experimental-json-modules deployment-check.js",
    "build-check": "node --experimental-json-modules build-check.js",
    "analyze": "vite build --mode analyze",
    "build:github": "node scripts/build.js",
    "check-size": "node scripts/check-page-size.js",
    "lint:frontend": "eslint src/ --fix",
    "fix-paths": "node scripts/fix-paths.js"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@supabase/supabase-js": "^2.49.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "16.5.0",
    "express": "^5.1.0",
    "swiper": "^11.0.5",
    "terser": "^5.29.1",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.3.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "vue-eslint-parser": "10.1.3"
  }
}
